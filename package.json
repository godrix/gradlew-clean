{
  "name": "gradlew-clean",
  "version": "0.0.0-development",
  "description": "Executes the './gradlew clean' command in the Android folder.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "gradlew-clean": "bin/index.js"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "tsc",
    "commit": "git-cz",
    "lint": "eslint './src/*.ts'",
    "lint:fix": "eslint './src/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "semantic-release": "semantic-release --branches main",
    "prepublishOnly": "npm run build",
    "prepare": "husky install"
  },
  "author": "Godrix",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/godrix/gradlew-clean"
  },
  "bugs": {
    "url": "https://github.com/godrix/gradlew-clean/issues"
  },
  "keywords": [
    "gradlew",
    "clean",
    "android",
    "build",
    "development",
    "tool",
    "script",
    "command",
    "CLI",
    "automation",
    "mobile",
    "app",
    "project",
    "development",
    "npm",
    "package",
    "utility",
    "deployment",
    "tooling",
    "build process"
  ],
  "dependencies": {
    "chalk": "4.1.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.39.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "8.0.3",
    "jest": "^29.5.0",
    "semantic-release": "^21.0.2",
    "ts-jest": "^29.1.0",
    "ts-standard": "^12.0.2",
    "typescript": "5.0.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
